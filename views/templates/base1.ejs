<table class="table table-bordered table-hover table-condensed">
<thead>
    <tr>
        <th>Name</th>
        <th>Score</th>
    </tr>
</thead>
<tbody>
        <% for(i in items){ %>
        <tr class="info">
            <td><a href="/link/<%= items[i]._id %>"><%= items[i].name %></a></td>
            <td><a href="/link/<%= items[i]._id %>"><%= items[i].score %></a></td>  
        </tr>
        <% } %>
</tbody>
</table>